/* Progressive Internet Explorer CSS3 readme http://css3pie.com/ */

.button  {border:none;padding:6px 20px 7px;margin:0;height:30px;font-size:12px;color:#333333;text-shadow:0 1px 1px #FFF;vertical-align:middle;overflow:visible;} 
.button, button {background-position:0px -110px, 100% -20px;}
.button:hover, button:hover {background-position:0px -140px, 100% -50px;}
.button:active, button:active {background-position:0px -170px, 100% -80px;}

.button {background:url("../images/interface.png") no-repeat 100% -20px !important;}
.button {-ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/btn-norm.png', sizingMethod='crop')";} /* Now lets make it IE8 Multi-background images */
.button {filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/btn-norm.png', sizingMethod='crop');} /* for IE7 and IE6 */
.button:hover {background:url("../images/interface.png") no-repeat 100% -50px !important;}
.button:hover {-ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/btn-hover.png', sizingMethod='crop')";} 
.button:hover {filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/btn-hover.png', sizingMethod='crop');} 
/* :active не работает для инпутов в IE :( */
.button:active {background:url("../images/interface.png") no-repeat 100% -50px !important;}
.button:active {-ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/btn-active.png', sizingMethod='crop')";} 
.button:active {filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/btn-active.png', sizingMethod='crop');}
.button {position:relative;behavior: url(css/PIE.htc);box-shadow:0px 1px 1px #DDD;} /* Note: this path is relative to the HTML file being viewed, not the CSS file it is called from. */

.inputbox , .cu-select {border:1px solid #CCCCCC;padding:6px 4px;margin:0;background:url("../images/interface.png") no-repeat 0px -330px;color:#333333;font-size:13px;vertical-align:middle;}

.inputbox {position:relative;behavior: url(css/PIE.htc);} 
.t-area {position:relative;behavior: url(css/PIE.htc);} 
.loginform {behavior: url(css/PIE.htc);box-shadow:3px 3px 1px #CCC;} 
.login-pane .active {behavior: url(css/PIE.htc);} 
.mainpage .loginform {box-shadow:none;}